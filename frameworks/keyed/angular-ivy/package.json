{
  "name": "angular-closure",
  "version": "1.0.0",
  "description": "An angular app using closure compiler",
  "js-framework-benchmark": {
    "frameworkVersionFromPackage": "@angular/core"
  },
  "scripts": {
    "postinstall": "ivy-ngcc",
    "start": "echo 'please start in root project'",
    "test": "karma start",
    "prengc": "rimraf tmp",
    "ngc": "ngc -p tsconfig.json",
    "preclosure": "rimraf dist",
    "closure": "google-closure-compiler --flagfile closure.conf",
    "build-prod": "npm run ngc && npm run closure",
    "rollup": "rollup -c rollup.config.js",
    "build-dev": "npm run ngc && npm run rollup"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "7.1.4",
    "@angular/compiler": "7.1.4",
    "@angular/compiler-cli": "7.1.4",
    "@angular/core": "7.1.4",
    "@angular/platform-browser": "7.1.4",
    "core-js": "2.6.1",
    "rxjs": "6.3.3",
    "tslib": "1.9.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "google-closure-compiler": "20181210.0.0",
    "http-server": "0.11.1",
    "rimraf": "2.6.2",
    "rollup": "0.68.1",
    "rollup-plugin-alias": "1.5.1",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-terser": "3.0.0",
    "rollup-plugin-uglify": "6.0.0",
    "typescript": "3.1.4",
    "yargs": "12.0.5"
  }
}
